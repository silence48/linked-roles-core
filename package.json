{
  "name": "linked-roles-core",
  "description": "core components for communi web",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/CommuniDAO/communi-core"
  },
  "author": "Julian Clatro <hola@julianclatro.com> (https://communidao.cc)",
  "contributors": [
    "Julian Clatro <hola@julianclatro.com> (https://communidao.cc)"
  ],
  "license": "MIT",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "watch-build": "tsc -w",
    "watch-test": "ava --watch",
    "dev": "concurrently 'npm:watch-*'",
    "build": "tsc",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "xo",
    "lint:md": "remark . -qfo",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test ava",
    "test-coverage": "cross-env NODE_ENV=test nyc npm run test"
  },
  "keywords": [
    "communi-core",
    "lass"
  ],
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@miniflare/d1": "latest",
    "@miniflare/shared": "latest",
    "@tsndr/cloudflare-worker-jwt": "^2.0.1",
    "@types/big.js": "^6.1.5",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.6.5",
    "ava": "latest",
    "browser-env": "^3.3.0",
    "codecov": "latest",
    "concurrently": "^8.2.0",
    "cross-env": "latest",
    "esbuild": "^0.15.0",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-config-xo": "latest",
    "eslint-plugin-ava": "latest",
    "eslint-plugin-compat": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-unicorn": "latest",
    "fixpack": "latest",
    "form-data": "^4.0.0",
    "husky": "latest",
    "joi": "latest",
    "jwt-decode": "^3.1.2",
    "lint-staged": "latest",
    "model-one": "latest",
    "node-fetch": "^2.6.7",
    "nyc": "latest",
    "prettier": "latest",
    "remark-cli": "^9.0.0",
    "remark-preset-github": "latest",
    "sodium-native": "^3.4.1",
    "stellar-base": "^8.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">= 10"
  },
  "bugs": {
    "url": "https://github.com/hacksur/funskill-core/issues",
    "email": "hola@julianclatro.com"
  },
  "homepage": "https://github.com/hacksur/funskill-core",
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "require": [
      "./test/_setup-browser-env.ts"
    ]
  },
  "volta": {
    "node": "17.9.1"
  }
}
